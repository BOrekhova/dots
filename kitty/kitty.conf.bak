font_family JetBrainsMono Nerd Font
font_size 15.0

cursor_blink_interval 0
confirm_os_window_close 0
enable_audio_bell no

tab_bar_edge top
tab_bar_style fade
tab_fade 0 1 1 1

map ctrl+t new_tab
map ctrl+w close_tab

# BEGIN_KITTY_THEME
# Grass
include current-theme.conf
# END_KITTY_THEME
# background #1d1d1d
# foreground #A7AEB8
